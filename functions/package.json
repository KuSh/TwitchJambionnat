{
  "name": "functions",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "scripts": {
    "build:watch": "tsc --watch",
    "build": "tsc",
    "check": "tsc --noEmit",
    "clean": "node -e \"fs.rmSync('lib', {force:true,recursive:true})\"",
    "deploy": "firebase deploy --only functions",
    "emulator": "firebase emulators:start --only functions",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "logs": "firebase functions:log",
    "serve": "npm-run-all build --parallel build:watch emulator",
    "shell": "firebase functions:shell",
    "start": "npm-run-all build --parallel build:watch shell"
  },
  "dependencies": {
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.4.0",
    "luxon": "^3.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tsconfig/node20": "^20.1.6",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.17.2",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  }
}
